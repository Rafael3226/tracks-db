{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug seed",
      "program": "${workspaceFolder}/node_modules/npm/bin/npm-cli.js",
      "args": ["run", "seed"],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "outFiles": [],
      "env": {
        "NODE_ENV": "development"
      },
      "stopOnEntry": false,
      "runtimeArgs": ["--nolazy"],
      "restart": false,
      "runtimeVersion": "default"
    }
  ]
}
